<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Combat Extended</li>
		<li>Cupro's Alloys (Continued)</li>
    </mods>
	<match Class="PatchOperationSequence">
	<success>Always</success>
		<operations>
		
		<!-- 5.56x45mm NATO -->
		<!-- FMJ -->
		<li Class="PatchOperationAdd">
		<xpath>/Defs/RecipeDef[defName="MakeAmmo_556x45mmNATO_FMJ"]/ingredients</xpath>
			<value>
				<li>
					<filter>
						<thingDefs>
							<li>Steel</li>
							<li>CAL_Cupronickel</li>
							<li>CAL_Bronze</li>
							<li>CAL_Brass</li>
						</thingDefs>
					</filter>
					<count>5</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>Steel</li>
							<li>CAL_Lead</li>
							<li>CAL_Plumchalcum</li>
						</thingDefs>
					</filter>
					<count>10</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>CAL_Brass</li>
						</thingDefs>
					</filter>
					<count>20</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>Chemfuel</li>
						</thingDefs>
					</filter>
					<count>5</count>
				</li>
			</value>
		</li>
		<li Class="PatchOperationAdd">
			<xpath>/Defs/RecipeDef[defName="MakeAmmo_556x45mmNATO_FMJ"]/fixedIngredientFilter</xpath>
			<value>
					<thingDefs>
						<li>Steel</li>
						<li>Chemfuel</li>
						<li>CAL_Brass</li>
						<li>CAL_Bronze</li>
						<li>CAL_Cupronickel</li>
						<li>CAL_Lead</li>
						<li>CAL_Plumchalcum</li>
					</thingDefs>
			</value>
		</li>
		
		</operations>
	</match>
</Operation>
</Patch>